{
	"presets": [
		"next/babel"
	],
	"plugins": [
		[
			"module-resolver",
			{
				"root": [
					"./"
				],
				"alias": {
					"components": "./components",
					"pages": "./pages",
					"commons": "./commons",
					"actions": "./actions",
					"reducers": "./reducers",
					"utils": "./utils",
					"constants": "./constants"
				}
			}
		]
	],
	"env": {
		"development": {
			"presets": [
				"next/babel"
			]
		},
		"production": {
			"presets": [
				"next/babel"
			]
		},
		"test": {
			"presets": [
				[
					"next/babel",
					{
						"preset-env": {
							"modules": "commonjs"
						}
					}
				]
			]
		}
	}
}
